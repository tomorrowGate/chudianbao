/* pages/compnents/ gallery.wxss */
@import "/app.wxss";
image{
    width: 100%;
    height: 100%;
}
.fix-page{
    display: flex;
    position: fixed;
    top:0;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100vw;
    height:100vh;
    background: rgba(0,0,0,0.2);
    z-index: 5;
}
.dialog-main{
    position: fixed;
    display: flex;
    flex-direction: column;
    width: 632rpx;
    height:832rpx;
    padding: 40rpx;
    left: 0px;
    right: 0px;
    top:0;
    bottom:0;
    margin: auto;
    background: #fff;
    transform: translateX(750px);
    z-index:6;
}
.dialog-main-brand{
    position: fixed;
    display: flex;
    flex-direction: column;
    width: 632rpx;
    height:632rpx;
    padding: 40rpx;
    left: 0px;
    right: 0px;
    top:0;
    bottom:0;
    margin: auto;
    background: #fff;
    transform: translateX(750px);
    z-index:6;
}
.dialog-conimg{
    display: flex;
    justify-content: center;
    height: 632rpx;
}
.gallery-text{
    display: flex;
    justify-content: space-between;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
}
.gallert-text-light{
    color: #999;
}
.button .btn{
    color: #fff;
    background-color: #1f9cfd;
    border-radius: 45rpx;
}